local Part = Script.parent
local TweenService = game:GetService("TweenService") 
local Pos = Vector3.new(-20, -6.5, -248) -- Change this to the position you want part to move to

local Moving_Info = TweenInfo.new(
	2, --the amount of seconds you want it to take
	Enum.EasingStyle.Quad,
	Enum.EasingDirection.InOut,
	100, --The amount of time you want it to repeat
	true, --Whether you want it to reverse back to the original position when it's done
	1 -- how much time should delay between each repitition
)

TweenService:Create(Part, Moving_Info, {Position = Pos}):Play()
